        android:name="it.unina.cinemates.ui.user.profile.other.ProfileFragment"
        android:label="fragment_other_user_profile"
        tools:layout="@layout/fragment_other_user_profile" />
    <fragment
        android:id="@+id/navigation_other_user_movie_list"
        android:name="it.unina.cinemates.ui.user.movielist.other.MovieListFragment"
        android:label="fragment_other_user_movie_list"
        tools:layout="@layout/fragment_other_user_movie_list" />
    <fragment
        android:id="@+id/navigation_other_user_other_lists"
        android:name="it.unina.cinemates.ui.user.customMovielists.other.CustomMovieListsFragment"
        android:label="fragment_other_user_other_lists"
        tools:layout="@layout/fragment_other_user_other_lists" />
    <fragment
        android:id="@+id/navigation_movies_from_actor"
        android:name="it.unina.cinemates.ui.movie.MoviesFromActorFragment"
        android:label="fragment_movies_from_actor"
        tools:layout="@layout/fragment_movies_from_actor" />
</navigation>