        android:name="it.unina.cinemates.ui.user.profile.logged.settings.ChangePasswordFragment"
        android:label="fragment_logged_user_change_password"
        tools:layout="@layout/fragment_logged_user_change_password" />
    <fragment
        android:id="@+id/navigation_logged_user_delete_account"
