        android:label="@string/bottom_bar_discover_lists"
        tools:layout="@layout/fragment_discover_lists" />
    <fragment
        android:id="@+id/navigation_suggest_movie"
        android:name="it.unina.cinemates.ui.suggestmovie.SuggestMovieFragment"
        android:label="fragment_suggest_movie"
        tools:layout="@layout/fragment_suggest_movie" />
    <fragment
        android:id="@+id/navigation_notification"
        android:name="it.unina.cinemates.ui.notification.NotificationFragment"
